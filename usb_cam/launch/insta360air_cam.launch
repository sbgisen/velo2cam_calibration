<launch>
  <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" >
    <param name="video_device" value="/dev/video4" />
    <param name="image_width" value="1472" />
    <param name="image_height" value="736" />
    <param name="pixel_format" value="mjpeg" />
    <param name="camera_frame_id" value="insta360air_cam" />
    <param name="io_method" value="mmap"/>
		<param name="contrast" value="125" />
		<param name="brightness" value="100" />
    <param name="autofocus" value="false" />
    <param name="camera_info_url" value="" />
		<param name="focus" value="20" />
    <param name="camera_name" value="insta360air_cam" />
  </node>


  <node name="image_view" pkg="image_view" type="image_view" respawn="false" >
    <remap from="image" to="/usb_cam/image_raw"/>
    <param name="autosize" value="true" />
  </node>


  <node name="sxs_stereo" pkg="side_x_side_stereo" type="side_x_side_stereo_node" respawn="false" output="screen" >
	 <param name="input_image_topic" value="/usb_cam/image_raw" />
   <param name="left_camera_info_url" value="file://$(find side_x_side_stereo)/config/insta360air_cam_left.yaml" />
   <param name="right_camera_info_url" value="file://$(find side_x_side_stereo)/config/insta360air_cam_right.yaml" />
  </node>
  

  <!-- <node name="image_view_right" pkg="image_view" type="image_view" respawn="false" >
    <remap from="image" to="/sxs_stereo/right/image_raw"/>
    <param name="autosize" value="true" />
  </node>
  <node name="image_view_left" pkg="image_view" type="image_view" respawn="false" >
    <remap from="image" to="/sxs_stereo/left/image_raw"/>
    <param name="autosize" value="true" />
  </node> -->

  <node name="image_proc_right" pkg="image_proc" type="image_proc" ns="sxs_stereo/right" respawn="false" >
  </node>
  <node name="image_proc_left" pkg="image_proc" type="image_proc" ns="sxs_stereo/left" respawn="false" >
  </node>


  <!-- <node name="image_view_right_rect" pkg="image_view" type="image_view" respawn="false">
    <remap from="image" to="/sxs_stereo/right/image_rect_color"/>
    <param name="autosize" value="true" />
  </node>
  <node name="image_view_left_rect" pkg="image_view" type="image_view" respawn="false">
    <remap from="image" to="/sxs_stereo/left/image_rect_color"/>
    <param name="autosize" value="true" />
  </node> -->




</launch>
